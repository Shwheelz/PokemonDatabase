  <div id="pokemon_form" class="col-md-6">
  <%= form_for(@pokemon) do |f| %>
    <% if @pokemon.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(@pokemon.errors.count, "error") %> prohibited this pokemon from being saved:</h2>

        <ul>
        <% @pokemon.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
        </ul>
      </div>
    <% end %>

    <div class="field">
      <%= f.label :species %><br>
      <%= f.text_field :species %>
    </div>
    <div class="field">
      <%= f.label :type1, "Type 1" %><br>
      <%= f.text_field :type1 %>
    </div>
    <div class="field">
      <%= f.label :type2, "Type 2" %><br>
      <%= f.text_field :type2 %>
    </div>
    <div class="field">
      <%= f.label :isShiny, "Shiny" %><br>
      <%= f.check_box :isShiny %>
    </div>
    <div class="field">
      <%= f.label :eggMoves, "Egg Moves" %><br>
      <%= f.text_field :eggMoves %>
    </div>
    <div class="field">
      <%= f.label :nature %><br>
      <%= f.text_field :nature %>
    </div>
    <div class="field">
      <%= f.label :ability %><br>
      <%= f.text_field :ability %>
    </div>
    <div class="field">
      <%= f.label :gender %><br>
      <%= f.text_field :gender %>
    </div>
    <div class="field">
      <%= f.label :pokeball %><br>
      <%= f.text_field :pokeball %>
    </div>
    <div class="field">
      <%= f.label :hiddenPower, "Hidden Power" %><br>
      <%= f.text_field :hiddenPower %>
    </div>
    <div class="field", "IVs">
      <%= f.label :ivs %><br>
      <%= f.text_field :ivs %>
    </div>
    <div class="field">
      <%= f.label :evs, "EVs" %><br>
      <%= f.text_field :evs %>
    </div>
    <div class="field">
      <%= f.label :item %><br>
      <%= f.text_field :item %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
  <% end %>
</div>